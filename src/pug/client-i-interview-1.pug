---
title: HelpersUnited - Client - Interview interview

pageClassName: p-agency-i-interview
pageClassNameNum: p-agency-i-interview-1
mode: client
---
extends shared/_layout

block header
  include ./components/_header-internal

block footer

block content
  div.body.body--start(class!= pageClassName)

    section#section.section.section--agency
      .section__wrapper
        .section__wrapper-left

          section#navigation.navigation.navigation--agency
            .navigation__wrapper

              ul.navigation__list
                +navigationItemAgency('0 is-active', 'interviews', 'Interviews', 'agency-interviews-1.html')
                +navigationItemAgency('2', 'pushCloud', 'Push-to-cloud', 'agency-push-cloud-1.html')

        .section__wrapper-right
          .section__wrapper-hscroll

            section#i-interview.i-interview.section-top

              .i-interview__wrapper

                .section-top__heading
                  .section-top__heading-left
                    if(mode === 'client')
                      div
                        p!= "Agency name"
                        span!= "01.01.19"
                    else
                      p!= "Alvin Brewer"
                  .section-top__heading-right
                    a(href="#" class="" title="")
                      span
                       i.icon-font.icon-close

                .i-interview__body

                  .i-interview__content.section-top__content
                    .i-interview__content-left.section-top__content-left

                      .menu
                        -
                          var listItemNavArr = [
                            {
                              href: 'agency-i-interview-1.html',
                              name: 'Biography'
                            },
                            {
                              href: 'agency-i-interview-2.html',
                              name: 'Personal data',
                            },
                            {
                              href: 'agency-i-interview-3.html',
                              name: 'Employment information',
                            },
                            {
                              href: 'agency-i-interview-4.html',
                              name: 'Household member',
                            },
                            {
                              href: 'agency-i-interview-5.html',
                              name: 'Other sources of income',
                            },
                            {
                              href: 'agency-i-interview-6.html',
                              name: 'Monthly expenses'
                            }
                          ];
                        ul.menu__list
                          each val, idx in listItemNavArr
                            li.menu__item
                              a.menu__btn(href= listItemNavArr[idx].href class=(idx === 0) ? 'is-active':'' title="")
                                span!= listItemNavArr[idx].name

                    .i-interview__content-right.section-top__content-right

                      .i-interview__field-wrapper
                        .i-interview__field-left
                          .i-interview__field
                            span.i-interview__field-label!= "First Name"
                            p.i-interview__field-value!= "Alvin"
                          .i-interview__field
                            span.i-interview__field-label!= "Middle Name"
                            p.i-interview__field-value!= "Brooks"
                          .i-interview__field
                            span.i-interview__field-label!= "Last Name"
                            p.i-interview__field-value!= "Brewer"
                          .i-interview__field
                            span.i-interview__field-label!= "DOB"
                            p.i-interview__field-value!= "12.12.1991"
                          .i-interview__field
                            span.i-interview__field-label!= "SNN"
                            p.i-interview__field-value!= "98874553"

                        .i-interview__field-right
                          .i-interview__field
                            span.i-interview__field-label!= "What is your gender?"
                            p.i-interview__field-value!= "Male"
                          .i-interview__field
                            span.i-interview__field-label!= "What is your marital status?"
                            p.i-interview__field-value!= "Married"
                          .i-interview__field
                            span.i-interview__field-label!= "What is your race?"
                            p.i-interview__field-value!= "Asian"
                          .i-interview__field
                            span.i-interview__field-label!= "Are you a veteran?"
                            p.i-interview__field-value!= "No"
                          .i-interview__field
                            span.i-interview__field-label!= "Can you walk without assistance?"
                            p.i-interview__field-value!= "No"
