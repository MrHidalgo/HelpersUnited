---
title: HelpersUnited - Agency - Document Libraries

pageClassName: p-agency-doc-lib
pageClassNameNum: p-agency-doc-lib-4
---
extends shared/_layout

block header
  include ./components/_header-internal

block footer

block content
  div.body.body--start(class!= pageClassName)

    section#section.section.section--agency
      .section__wrapper
        .section__wrapper-left

          section#navigation.navigation.navigation--agency
            .navigation__wrapper

              ul.navigation__list
                +navigationItemAgency('0', 'interviews', 'Interviews', 'agency-interviews-1.html')
                +navigationItemAgency('1', 'staff', 'Staff', 'agency-staff-1.html')
                +navigationItemAgency('2', 'pushCloud', 'Push-to-cloud', 'agency-push-cloud-1.html')
                +navigationItemAgency('3', 'meetingSchedule', 'Meeting Schedule', 'agency-meeting-schedule-1.html')
                +navigationItemAgency('4 is-active', 'documentLibraries', 'Document libraries', 'agency-document-libraries-1.html')
                +navigationItemAgency('5', 'qa', 'Q&A', 'agency-qa.html')
                +navigationItemAgency('6', 'videoTutorials', 'Video tutorials', 'agency-video-1.html')

        .section__wrapper-right.section__wrapper-right--two
          .section__wrapper-right--left

            include components/common/_tabs

            section#doc-lib.doc-lib

              .box__cover
                each val, idx in [1,2,3,4,5]
                  +sectionBox('dl', 'Document name', 'PDF', '', true, 'agency-document-libraries-5.html')

          .section__wrapper-right--right

            .doc-lib__section
              .doc-lib__section-header
                div
                  p!= "Upload PDF document"

                div
                  a(href="#" class="" title="")
                    i.icon-font.icon-close

              .doc-lib__section-body.doc-lib__section-body--start

                .doc-lib__section-content

                  .doc-lib__section-progress.doc-lib__section-progress--full
                    span
                    p!= "Uploaded"

                  .doc-lib__section-preview

                  -
                    var dataFormObj = {
                    inputName: {
                    classMod: "",
                    uniqueName: "document_name",
                    labelVal: "Document name",
                    inputType: "",
                    inputClassMod: "",
                    placeholderVal: "",
                    val: ""
                    }
                    };

                  .doc-lib__section-form.doc-lib__section-form--one
                    .c-form
                      +formInput(dataFormObj.inputName)

                      .c-form__group.c-form__group--two
                        .c-form__group-col.c-form__group-col-0
                          a.doc-lib__section-delete(href="agency-document-libraries-1.html" class="" title="")
                            i.icon-font.icon-delete
                            span!= "Delete"
                        .c-form__group-col.c-form__group-col-1
                          +btn('c-btn c-btn--bg', 'agency-document-libraries-1.html', 'Add')
