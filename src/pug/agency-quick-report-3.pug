---
title: HelpersUnited - Agency - Quick Report

pageClassName: p-agency-q-report
pageClassNameNum: p-agency-q-report-3
---
extends shared/_layout

block header
  include ./components/_header-internal

block footer

block content
  div.body.body--start(class!= pageClassName)

    section#section.section.section--agency
      .section__wrapper
        .section__wrapper-left

          section#navigation.navigation.navigation--agency
            .navigation__wrapper

              ul.navigation__list
                +navigationItemAgency('0 is-active', 'interviews', 'Interviews', 'agency-interviews-1.html')
                +navigationItemAgency('1', 'staff', 'Staff', 'agency-staff-1.html')
                +navigationItemAgency('2', 'pushCloud', 'Push-to-cloud', 'agency-push-cloud-1.html')
                +navigationItemAgency('3', 'meetingSchedule', 'Meeting Schedule', 'agency-meeting-schedule-1.html')
                +navigationItemAgency('4', 'documentLibraries', 'Document libraries', 'agency-document-libraries-1.html')
                +navigationItemAgency('5', 'qa', 'Q&A', 'agency-qa.html')
                +navigationItemAgency('6', 'videoTutorials', 'Video tutorials', 'agency-video-1.html')

        .section__wrapper-right
          .section__wrapper-hscroll

            section#q-report.q-report.section-top

              .q-report__wrapper

                .section-top__heading
                  .section-top__heading-left
                    p!= "Alvin Brewer"
                  .section-top__heading-right
                    a(href="#" class="" title="")
                      span
                       i.icon-font.icon-close

                .q-report__body

                  include components/common/_tabs

                  .q-report__content.section-top__content
                    .q-report__content-left.section-top__content-left

                      .menu
                        -
                          var listItemNavArr = [
                          {
                          href: 'agency-quick-report-1.html',
                          name: 'Regional averages',
                          },
                          {
                          href: 'agency-quick-report-2.html',
                          name: 'Dynamics',
                          },
                          {
                          href: 'agency-quick-report-3.html',
                          name: 'Support history',
                          },
                          {
                          href: 'agency-quick-report-4.html',
                          name: 'Suggestions'
                          }
                          ];
                        ul.menu__list
                          each val, idx in listItemNavArr
                            li.menu__item
                              a.menu__btn(href= listItemNavArr[idx].href class=(idx === 2) ? 'is-active': '' title="")
                                span!= listItemNavArr[idx].name

                    .q-report__content-right.section-top__content-right

                      .q-report__table
                        .q-report__table-title
                          div
                            p!= "Alvin Brewer"
                          div
                            a(href="#" class="" title="")
                              i.icon-font.icon-add

                        .q-report__table-head
                          -
                            var quickReportTableBtnEmpty = true,
                              quickReportTable1 = true;
                          include components/common/_tabs

                        .q-report__table-body
                          .q-report__table-tr
                            .q-report__table-th
                              span!= "Support name"
                            .q-report__table-th
                              span!= "Requested Date"
                            .q-report__table-th
                              span!= "Amount"
                            .q-report__table-th
                              span!= "Agency Name"

                          .q-report__table-tr
                            .q-report__table-td
                              span!= "Water"
                            .q-report__table-td
                              span!= "12.01.18"
                            .q-report__table-td
                              span!= "$200"
                            .q-report__table-td.q-report__table-td--more
                              div
                                span!= "Helpers"
                              div
                                .c-dropdown
                                  a.c-dropdown__btn(href="#" class="" title="" dropdown-js)
                                    i.icon-font.icon-more
                                  div.c-dropdown__content
                                    ul
                                      li
                                        a.c-dropdown__link(href="#" class="" title="")
                                          p
                                            i.icon-font.icon-edit
                                          span!= "Edit"
                                      li
                                        a.c-dropdown__link(href="#" class="" title="")
                                          p
                                            i.icon-font.icon-delete
                                          span!= "Delete"

                      .q-report__table.q-report__table--edit
                        .q-report__table-title
                          div
                            p!= "Bruce Wagner Â· Household member"
                          div
                            a(href="#" class="" title="")
                              i.icon-font.icon-add

                        .q-report__table-head
                          -
                            var quickReportTable1 = false,
                              quickReportTable2 = true;
                          include components/common/_tabs

                        .q-report__table-body
                          .q-report__table-tr
                            .q-report__table-th
                              span!= "Support name"
                            .q-report__table-th
                              span!= "Requested Date"
                            .q-report__table-th
                              span!= "Amount"
                            .q-report__table-th
                              span!= "Agency Name"

                          .q-report__table-tr.q-report__table-tr--check
                            .q-report__table-td
                              input(type="text" placeholder="Water")
                            .q-report__table-td
                              input(type="text" placeholder="12.01.18")
                            .q-report__table-td
                              input(type="text" placeholder="$200")
                            .q-report__table-td.q-report__table-td--double
                              div
                                span!= "Helpers"
                              div
                                a(href="#" class="" title="")
                                  i.icon-font.icon-check

                          .q-report__table-tr.q-report__table-tr--del
                            .q-report__table-td
                              -
                                var dataFormObj = {
                                  selectFieldCustom1: {
                                    classMod: "staff__table-select",
                                    uniqueName: "select_status",
                                    labelVal: "",
                                    selectClassMod: "",
                                    selectOption: [
                                    'Some option 1',
                                    'Some option 2',
                                    'Some option 3'
                                    ],
                                    placeholderVal: "Support name"
                                  },
                                  selectFieldCustom2: {
                                    classMod: "staff__table-select",
                                    uniqueName: "select_status",
                                    labelVal: "",
                                    selectClassMod: "",
                                    selectOption: [
                                    'Some option 1',
                                    'Some option 2',
                                    'Some option 3'
                                    ],
                                    placeholderVal: "Approve date"
                                  }
                                };

                              .c-form
                                +formSelectCustom(dataFormObj.selectFieldCustom1)
                            .q-report__table-td
                              .c-form
                                +formSelectCustom(dataFormObj.selectFieldCustom2)
                            .q-report__table-td
                              input(type="text" placeholder="Amount")
                            .q-report__table-td.q-report__table-td--double
                              div
                              div
                                a(href="#" class="" title="")
                                  i.icon-font.icon-close
