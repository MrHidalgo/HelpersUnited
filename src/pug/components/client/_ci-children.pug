#ci.ci.ci--children

  h2.ci__heading!= "5. Household member"
  h3.ci__subheading!= "5.2. Children"

  p.ci__title!= "How many children, 16 years and younger, reside in the home?"

  .ci__row
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-1', 'ci-children-home', '1')(checked="checked")
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-2', 'ci-children-home', '2')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-3', 'ci-children-home', '3')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-4', 'ci-children-home', '4')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-5', 'ci-children-home', '5')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-6', 'ci-children-home', '6')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-7', 'ci-children-home', '7')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-8', 'ci-children-home', '8')
    .ci__col-1-3
      +clientInterviewRadioBtn('ci-children-home-9', 'ci-children-home', '9')
    .ci__col-1-3.ci__col-1-3--center
      +clientInterviewRadioBtn('ci-children-home-10', 'ci-children-home', '10')

  hr.ci__divider
  p.ci__title!= "Collect following information"

  +formInput(dataFormObj.inputName1)
  +formInput(dataFormObj.inputName2)
  +formInput(dataFormObj.inputName3)
  +formInput(dataFormObj.inputName4)

  hr.ci__divider
  p.ci__title!= "Date of Birth"

  .ci__row
    .ci__col-1-3
      +formSelectCustom(dataFormObj.selectFieldCustomDBDay)
    .ci__col-1-3
      +formSelectCustom(dataFormObj.selectFieldCustomDBMonth)
    .ci__col-1-3
      +formSelectCustom(dataFormObj.selectFieldCustomDBYear)

  hr.ci__divider
  p.ci__title!= "What is the child gender?"

  .ci__row
    .ci__col-1-2
      +clientInterviewRadioBtn('ci-gender-male', 'ci-gender', 'Male')(checked="checked")
    .ci__col-1-2
      +clientInterviewRadioBtn('ci-gender-female', 'ci-gender', 'Female')

  hr.ci__divider
  p.ci__title!= "What is your race?"

  .ci__row
    .ci__col-1
      +clientInterviewRadioBtn('ci-race-0', 'ci-race', 'American Indian or Alaska Native')(checked="checked")
    .ci__col-1
      +clientInterviewRadioBtn('ci-race-1', 'ci-race', 'Asian')
    .ci__col-1
      +clientInterviewRadioBtn('ci-race-2', 'ci-race', 'Black or African American')
    .ci__col-1
      +clientInterviewRadioBtn('ci-race-3', 'ci-race', 'Native Hawaiian or Other Pacific Islander')
    .ci__col-1
      +clientInterviewRadioBtn('ci-race-4', 'ci-race', 'White')

  hr.ci__divider
  p.ci__title!= "Relationship"

  .ci__row
    .ci__col-1-2
      +clientInterviewRadioBtn('ci-Relationship-0', 'ci-Relationship', 'Nephew')(checked="checked")
    .ci__col-1-2
      +clientInterviewRadioBtn('ci-Relationship-1', 'ci-Relationship', 'Nephew')
    .ci__col-1-2
      +clientInterviewRadioBtn('ci-Relationship-2', 'ci-Relationship', 'Son / Daughter')
    .ci__col-1-2
      +clientInterviewRadioBtn('ci-Relationship-3', 'ci-Relationship', 'No relation')

  .ci__row.ci__row--padding-top
    .ci__col-1
      +formInput(dataFormObj.inputName5)

  hr.ci__divider
  .ci__btn-wrapper
    a.ci__btn(href="client-interview-10.html" class="" title="")!= "Next"

