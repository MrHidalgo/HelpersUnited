---
title: HelpersUnited - Agency - Document Libraries

pageClassName: p-agency-doc-lib
pageClassNameNum: p-agency-doc-lib-2
---
extends shared/_layout

block header
  include ./components/_header-internal

block footer

block content
  div.body.body--start(class!= pageClassName)

    section#section.section.section--agency
      .section__wrapper
        .section__wrapper-left

          section#navigation.navigation--agency
            .navigation__wrapper

              ul.navigation__list
                +navigationItemAgency('0', 'interviews', 'Interviews')
                +navigationItemAgency('1', 'staff', 'Staff')
                +navigationItemAgency('2', 'pushCloud', 'Push-to-cloud')
                +navigationItemAgency('3', 'meetingSchedule', 'Meeting Schedule')
                +navigationItemAgency('4 is-active', 'documentLibraries', 'Document libraries')
                +navigationItemAgency('5', 'qa', 'Q&A')
                +navigationItemAgency('6', 'videoTutorials', 'Video tutorials')

        .section__wrapper-right.section__wrapper-right--two
          .section__wrapper-right--left

            include components/common/_tabs

            section#doc-lib.doc-lib

              .box__cover
                +sectionBox('dl', 'Document name', 'PDF', '', true)
                +sectionBox('dl', 'Document name', 'PDF', '', true)
                +sectionBox('dl', 'Document name', 'PDF', '', true)
                +sectionBox('dl', 'Document name', 'PDF', '', true)
                +sectionBox('dl', 'Document name', 'PDF', '', true)

          .section__wrapper-right--right

            .doc-lib__section
              .doc-lib__section-header
                div
                  p!= "Upload PDF document"

                div
                  a(href="#" class="" title="")
                    i.icon-font.icon-close

              .doc-lib__section-body

                a.doc-lib__empty(href="#")
                  i.icon-font.icon-upload
                  p!= "Select file to upload"
                  span!= "Or drag and drop video file"
