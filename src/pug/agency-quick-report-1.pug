---
title: HelpersUnited - Agency - Quick Report

pageClassName: p-agency-q-report
pageClassNameNum: p-agency-q-report-1
---
extends shared/_layout

block header
  include ./components/_header-internal

block footer

block content
  div.body.body--start(class!= pageClassName)

    section#section.section.section--agency
      .section__wrapper
        .section__wrapper-left

          section#navigation.navigation--agency
            .navigation__wrapper

              ul.navigation__list
                +navigationItemAgency('0 is-active', 'interviews', 'Interviews')
                +navigationItemAgency('1', 'staff', 'Staff')
                +navigationItemAgency('2', 'pushCloud', 'Push-to-cloud')
                +navigationItemAgency('3', 'meetingSchedule', 'Meeting Schedule')
                +navigationItemAgency('4', 'documentLibraries', 'Document libraries')
                +navigationItemAgency('5', 'qa', 'Q&A')
                +navigationItemAgency('6', 'videoTutorials', 'Video tutorials')

        .section__wrapper-right

          section#q-report.q-report.section-top

            .q-report__wrapper

              .section-top__heading
                .section-top__heading-left
                  p!= "Alvin Brewer"
                .section-top__heading-right
                  a(href="#" class="" title="")
                    span
                     i.icon-font.icon-close

              .q-report__body

                include components/common/_tabs

                .q-report__content.section-top__content
                  .q-report__content-left.section-top__content-left

                    .menu
                      -
                        var listItemNavArr = [
                          'Regional averages',
                          'Dynamics',
                          'Support history',
                          'Suggestions'
                        ];
                      ul.menu__list
                        each val, idx in listItemNavArr
                          li.menu__item
                            a.menu__btn(href="#" class=(idx === 0) ? 'is-active':'' title="")
                              span!= val

                  .q-report__content-right.section-top__content-right

                    .q-report__block-wrapper
                      .q-report__block.q-report__block-0
                        .q-report__block-head
                          p!= "Type of expense"

                        .q-report__block-body
                          -
                            var blockRowName = [
                              'Rent',
                              'Food',
                              'Cleaning, etc',
                              'Cable',
                              'Electric',
                              'Water / Sewer',
                              'Home phone',
                              'Cell phone',
                              'Natural gas',
                              'Furniture loan',
                              'Electronics loan',
                              'Applicane loan',
                              'Car',
                              'Car Insurance',
                              'Gasoline',
                              'Child care',
                              'Child support',
                              'Med insurance',
                              'Med loans',
                              'Meds, docs, etc',
                              'Credit cards',
                              'Other loans',
                              'Clothing',
                              'Misc'
                            ];
                          each val, idx in blockRowName
                            .q-report__block-row
                              p!= val

                      .q-report__block.q-report__block-1
                        .q-report__block-head
                          p!= "Client expenses"

                        .q-report__block-body
                          each val, idx in blockRowName
                            .q-report__block-row.q-report__block-row--up
                              .q-report__block-col
                                p!= "$120"
                              .q-report__block-col
                                span!= "+ $20"
                                span
                                  i.icon-font.icon-triangle-up
                                span!= "+5%"

                      .q-report__block.q-report__block-2
                        .q-report__block-head
                          p!= "Regional average expenses"

                        .q-report__block-body
                          each val, idx in blockRowName
                            .q-report__block-row.q-report__block-row--down
                              .q-report__block-col
                                p!= "$120"
                              .q-report__block-col
                                span!= "- $20"
                                span
                                  i.icon-font.icon-triangle-down
                                span!= "-2%"